package sv.edu.udb.guia2;
import java.util.Scanner;
public class Ejercicio1 {

	public static void main(String[] args) {
		double num1;
		double num2;
		double resultado;
		
		 System.out.println("Vamos a dividir 2 numeros");
		Scanner scanner = new Scanner(System.in);
		
		System.out.print("ingrese numero A  :");
		 
		 num1 = scanner.nextFloat();
		
		System.out.print("ingrese numero B :");
		 num2 = scanner.nextFloat();
		if (num2>0){
			resultado =( num1/num2);
			System.out.println("=>la resuesta es " + resultado);
		 }else {
			 System.out.println("sintax error");
		 }
				
		}
		
	}
